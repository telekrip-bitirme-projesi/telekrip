<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dashboard.telekrip">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.RegisterActivity"
            android:launchMode="singleInstance">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.AccountVerificationActivity"
            android:launchMode="singleInstance" />
        <activity
            android:name=".Activity.MainActivity"
            android:launchMode="singleInstance" />
        <activity
            android:name=".Activity.ChatActivity"
            android:launchMode="singleInstance" />
        <activity
            android:name=".Activity.UserPanelActivity"
            android:launchMode="singleInstance" />
        <activity
            android:name=".Activity.ChangePasswordActivity"
            android:launchMode="singleInstance" />
        <activity android:name=".Activity.AddUserActivity" />
        <activity android:name=".Activity.AddUserDetailActivity"></activity>
    </application>

</manifest>